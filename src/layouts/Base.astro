---
import '../global.css'

export interface Props {
	title: string;
}

const { title } = Astro.props;
---

<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="UTF-8" />
	<meta name="viewport" content="width=device-width" />
	<link rel="shortcut icon" href="/favicon/favicon.ico" type="image/x-icon">
	<meta name="generator" content={Astro.generator} />
	<title>{title}</title>
	<link rel="apple-touch-icon" sizes="180x180" href="/favicon/apple-touch-icon.png">
	<link rel="icon" type="image/png" sizes="32x32" href="/favicon/favicon-32x32.png">
	<link rel="icon" type="image/png" sizes="16x16" href="/favicon/favicon-16x16.png">
	<link rel="manifest" href="/favicon/site.webmanifest">
	<link rel="mask-icon" href="/favicon/safari-pinned-tab.svg" color="#5bbad5">
	<meta name="msapplication-TileColor" content="#00aba9">
	<meta name="theme-color" content="#ffffff">
	<meta name="description" content="🏆 All current collection of github achievements 🏆">
	<link rel="canonical" href="">
	<meta name="application-name" content="GitHub-Profile-Achievements">
	<meta name="apple-mobile-web-app-title" content="GitHub-Profile-Achievements">
	<meta name="msapplication-TileImage" content="/favicon/mstile-144x144.png">
	<meta name="msapplication-config" content="/favicon/browserconfig.xml">
	<meta property="og:url" content="">
	<meta property="og:type" content="website">
	<meta property="og:title" content="GitHub-Profile-Achievements">
	<meta property="og:description" content="🏆 All current collection of github achievements 🏆">
	<meta property="og:image" content="/favicon/android-chrome-512x512.png">
	<meta property="og:locate" content="es_MX">
	<meta property="og:site_name" content="GitHub-Profile-Achievements">
</head>
<body>
	<slot />
	<script type="module" src="https://unpkg.com/ionicons@5.5.2/dist/ionicons/ionicons.esm.js"></script>
	<script nomodule src="https://unpkg.com/ionicons@5.5.2/dist/ionicons/ionicons.js"></script>
</body>
</html>
